{
    "name": "ts-node-starter",
    "version": "1.0.0",
    "description": "Base environment for run TypeScript of use ts-node.",
    "main": "main.ts",
    "scripts": {
        "main": "ts-node src/main.ts",
        "lint": "eslint --fix src/**/*.{ts,vue}",
